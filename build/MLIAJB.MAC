    STR    "MCSKIP"
    RL    DCOMMA,OF(191*LNM+146*LCH)
    CON    1
    CON    1
    CON    7
    HASH    "MCINS"
    CON    0
    CON    OF(5*LCH)
    STR    "MCINS"
    RL    DCOMMA,OF(184*LNM+141*LCH)
    CON    1
    CON    1
    CON    6
    HASH    "MCWARN"
    CON    0
    CON    OF(6*LCH)
    STR    "MCWARN"
    RL    DNL,OF(181*LNM+136*LCH)
