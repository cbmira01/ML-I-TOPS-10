    CON    OF(6*LCH)
    STR    "MCLENG"
CWITHS:    WTHS
    CON    OF(1*LCH)
    STR    "("
    RL    DRBRAC,OF(73*LNM+43*LCH)
    CON    1
    CON    0
    CON    13
    HASH    "MCSUB"
    CON    0
    CON    OF(5*LCH)
    STR    "MCSUB"
    WTHS
    CON    OF(1*LCH)
    STR    "("
    RL    DCOM1,OF(56*LNM+35*LCH)
    CON    1
    CON    0
    CON    14
    HASH    "MCSTOP"
    CON    0
    CON    OF(6*LCH)
    STR    "MCSTOP"
    RL    DNL,OF(65*LNM+33*LCH)
    CON    1
    CON    1
    CON    15





    NB    "These are all chained for use by ALTER macro"

DELCHN:    RL    ACAS,OF(4*LNM+4*LCH)
DVARS:    RL    DAS,OF(4*LNM+4*LCH)
    CON    OF(4*LCH)
    STR    "VARS"
    RL    DAS,OF(2*LNM+0*LCH)
ACAS:    RL    ACSSAS,OF(4*LNM+2*LCH)
DAS:    RL    DSSAS,OF(4*LNM+2*LCH)
    CON    OF(2*LCH)
    STR    "AS"
    RL    DNL,OF(54*LNM+27*LCH)
ACSSAS:    RL    ACIF,OF(4*LNM+4*LCH)
DSSAS:    CON    0
    CON    OF(4*LCH)
    STR    "SSAS"
    RL    DNL,OF(50*LNM+23*LCH)

ACIF:    RL    ACUNLE,OF(4*LNM+2*LCH)
DIF:    RL    DUNLES,OF(4*LNM+2*LCH)
    CON    OF(2*LCH)
    STR    "IF"
    RL    DGE,OF(6*LNM+6*LCH)
ACUNLE:    RL    ACGE,OF(4*LNM+6*LCH)
DUNLES:    RL    DNL,OF(44*LNM+21*LCH)
    CON    OF(6*LCH)
    STR    "UNLESS"
    RL    DGE,OF(2*LNM+0*LCH)
ACGE:    RL    ACGR,OF(4*LNM+2*LCH)
DGE:    RL    DGR,OF(4*LNM+2*LCH)
    CON    OF(2*LCH)
    STR    "GE"
    RL    DNL,OF(38*LNM+13*LCH)
ACGR:    RL    ACEN,OF(4*LNM+2*LCH)
DGR:    RL    DEN,OF(4*LNM+2*LCH)
    CON    OF(2*LCH)
    STR    "GR"
    RL    DNL,OF(34*LNM+11*LCH)
ACEN:    RL    ACBC,OF(4*LNM+2*LCH)
DEN:    RL    DBC,OF(4*LNM+2*LCH)
    CON    OF(2*LCH)
    STR    "EN"
    RL    DNL,OF(30*LNM+9*LCH)
ACBC:    RL    ACEQ,OF(4*LNM+2*LCH)
DBC:    RL    DEQ,OF(4*LNM+2*LCH)
    CON    OF(2*LCH)
    STR    "BC"
    RL    DNL,OF(26*LNM+7*LCH)
ACEQ:    RL    ACTO,OF(4*LNM+1*LCH)
DEQ:    CON    0
    CON    OF(1*LCH)
    STR    "="
    RL    DNL,OF(22*LNM+6*LCH)

ACTO:    RL    ACOM1,OF(4*LNM+2*LCH)
DTO:    CON    0
    CON    OF(2*LCH)
    STR    "TO"
    RL    DNL,OF(18*LNM+4*LCH)

ACOM1:    RL    ACOM2,OF(4*LNM+1*LCH)
DCOM1:    CON    0
    CON    OF(1*LCH)
    STR    ","
    RL    DCOM2,OF(2*LNM+0*LCH)
ACOM2:    RL    ACRBRA,OF(4*LNM+1*LCH)
DCOM2:    CON    0
    CON    OF(1*LCH)
    STR    ","
    RL    DRBRAC,OF(2*LNM+0*LCH)
ACRBRA:    RL    ACOMMA,OF(4*LNM+1*LCH)
DRBRAC:    CON    0
    CON    OF(1*LCH)
    STR    ")"
    CON    0

ACOMMA:    RL    ACNL,OF(4*LNM+1*LCH)
DCOMMA:    RL    DNL,OF(4*LNM+1*LCH)
    CON    OF(1*LCH)
    STR    ","
    RL    DNL,OF(2*LNM+0*LCH)
ACNL:    RL    LAYCHN,OF(4*LNM+1*LCH)
DNL:    CON    0
    CON    OF(LCH)
    NCH    NLREP
    CON    0

    NB    "Chain of layout characters"

LAYCHN:    RL    KNL,OF(4*LNM+5*LCH)
    RL    RSPACE,OF(46*LNM+40*LCH)
    CON    OF(5*LCH)
    STR    "SPACE"
    CON    0
KNL:    RL    KTAB,OF(4*LNM+2*LCH)
    RL    LAYREP,OF(42*LNM+32*LCH)
    CON    OF(2*LCH)
    STR    "NL"
    CON    0
KTAB:    RL    KSL,OF(4*LNM+3*LCH)
    RL    RTAB,OF(38*LNM+31*LCH)
    CON    OF(3*LCH)
    STR    "TAB"
    CON    0
KSL:    RL    KSPACS,OF(4*LNM+2*LCH)
    RL    RSL,OF(34*LNM+29*LCH)
    CON    OF(2*LCH)
    STR    "SL"
    CON    0
KSPACS:    RL    ACN,OF(4*LNM+6*LCH)
KSPS1:    RL    SPCSRP,OF(30*LNM+24*LCH)
    CON    OF(6*LCH)
    STR    "SPACES"
    CON    0

    NB    "Node marker"

ACN:    RL    KEYCHN,OF(4*LNM+1*LCH)
    CON    0
    CON    OF(LCH)
KNREP:    STR    "N"
    CON    0

    NB    "Keyword chain -- subchain of delimiter chain"

KEYCHN:    RL    ACWHS,OF(4*LNM+4*LCH)
    CON    0
    CON    OF(4*LCH)
    STR    "WITH"
    CON    0
ACWHS:    RL    ACOPT,OF(4*LNM+5*LCH)
KWITHS:    CON    1
    CON    OF(5*LCH)
    STR    "WITHS"
    CON    0
ACOPT:    RL    ACOR,OF(4*LNM+3*LCH)
KOPT:    CON    2
    CON    OF(3*LCH)
    STR    "OPT"
    CON    0
ACOR:    RL    ACALL,OF(4*LNM+2*LCH)
KOR:    CON    3
    CON    OF(2*LCH)
    STR    "OR"
    CON    0
ACALL:    RL    ACLAST,OF(4*LNM+3*LCH)
KALL:    CON    4
    CON    OF(3*LCH)
    STR    "ALL"
    CON    0
ACLAST:    CON    0

    NB    "Dummy last entry"

DUMMY:    CON    0
    CON    0




SPCSRP:    NCH    SPREP


    NB    "MD tables --- hash-tables and error block"

LAYREP:    NCH    NLREP
RTAB:    NCH    TABREP
RSL:    NCH    SLREP
RSPACE:    NCH    SPREP

GHSHTB:    THASH
    CON    0
    CON    0
    CON    0
    CON    0
    CON    7




    NB    "MI-logic of ML/I --- continued"
    NB    "Part 2 -- Program sections"
    NB    "Version of Aug 24 1971"
    NB    "Updated 8th January 1985  R.D. Eager"
    NB    "Updated 13th January 1986  R.D. Eager"




BEGIN::    LAV    FFPT,X

    NB    "Reserve ERBLOC,HASHTB,GHSHTB..."
    NB    "...and 10 permanent variables"

    STV    ETEMPT,P
    AAL    OF(7*LNM)
    STV    TEMPT,P
    AAL    OF(11*LNM)
    STV    PVARPT,P
    SAV    ETEMPT
    GOSUB    BUMPFF,637
    LAL    10
    STV    PVNUM,P
    FSTK
    LAV    LFPT,X
    STV    ENDPT,X
    LAL    7
    STV    GLBWSW,X

    NB    "Zeroise permanent variables"

ZPLP:    BUMP    TEMPT,OF(LNM)
    LAV    TEMPT,X
    CAV    PVARPT,A
    GOEQ    SETMKS,3,X,X
    LAL    0
    STI    TEMPT,X
    GO    ZPLP,-7,X,X

    NB    "Set values of special markers"

SETMKS:    LAA    CWITHS,C
    STV    TEMPT,X
    LAI    TEMPT,X
    STV    WTHSMK,P
    SAL    1
    STV    WITHMK,P
    SAL    1
    STV    EXCLMK,P
    SAL    1
    STV    SPCSMK,X
    LAL    1
    STV    NLSW,P
    STV    LINECT,X
    LAL    0
    STV    LABPT,P
    STV    STOPPT,P
    STV    TVARPT,P
    STV    ARGPT,P
    STV    OPLEV,P
    STV    NESTLV,P
    STV    SKIPLV,P
    STV    INVOCT,P
    STV    LEVEL,P
    STV    TLINCT,P
    STV    SKVAL,P
    STV    BINDIC,P
    STV    DBUGSW,P
    BSTK
    LAA    GHSHTB,C
    STV    HASHPT,X
    GOSUB    STKHSH,1137

    NB    "Set limits on locals"

    LAV    HASHPT,X
    AAL    OF(LHV)
    STV    TEMPT,X
LIMLP:    LAV    ENDPT,X
    STI    TEMPT,X
    BUMP    TEMPT,OF(LNM)
    LAI    TEMPT,X
    CAL    0
    GOEQ    LIMLP,-6,X,X
    LAA    KNREP,C
    STV    KNPT,X






    NB    "Start of logic -- Main scanning routine"

    LAV    PVARPT,X
    STV    FFPT,P
    AAL    OF(LNM-LCH)
    STV    SPT,X
    LAV    PVNUM,X
    FSTK
    LAV    FFPT,X
    STV    INFFPT,X
    LAV    PVNUM,X
    MULTL    OF(LNM)
    STV    TEMP,X
    LAV    PVARPT,X
    SAV    TEMP
    STV    STFFPT,X

BSNEXT:    GOSUB    SMSKSW,1105
    GOSUB    SBSTPL,1056
    CLEAR    BESPT
BSLOOP:    GOSUB    GTATOM,842
    LCI    IDPT,X
    CCN    STOPCD
    GOEQ    MNSTOP,67,X,X
    LAV    NESTLV,X
    CAL    0
    GOEQ    BSNAME,13,X,X

    NB    "Search delimiter chain"

    CLEAR    HTABPT
    LAV    DELPT,X
    GOSUB    LOSCHN,2805
    GO    MACERR,2429,X,C
CHN1:    LAV    CHANPT,X
    GOSUB    CMPARE,590
    GO    BSC1,1,X,C
    GOSUB    TEBEST,1141
BSC1:    GOSUB    LOECHN,2807
    GO    CHN1,-6,X,C
    LAV    MASKSW,X
    CAL    5
    GOEQ    ENEXCL,472,X,X

    NB    "Search chain of names"

BSNAME:    GOSUB    MDFIND,X
BSN1:    LAI    HTABPT,X
    STV    CHANPT,X
BSHASH:    LAV    CHANPT,X
    CAL    0
    GOEQ    BSSWIT,8,X,X
    LAV    CHANPT,R
    AAL    OF(LNM)
    GOSUB    CMPARE,574
    GO    BSH1,1,X,C
    GOSUB    TEBEST,1125
BSH1:    LAI    CHANPT,X
    STV    CHANPT,X
    GO    BSHASH,-11,X,X

BSSWIT:    GOADD    BESTPL
    GO    MNOUT,9,X,T
    GO    CALLO,134,X,T
    GO    WARNO,94,X,T
    GO    CALLO,132,X,T
    GO    SKIPPO,33,X,T
    GO    DELLO,170,X,T
    GO    BSLOOP,-42,X,T
    GO    WRNERR,103,X,T
    GO    SASTAK,15,X,T
    GO    STOPO,115,X,T


    NB    "Add atom to output text"

MNOUT:    LAV    IDPT,X
    AAV    IDLEN
    STV    TEMPT,X
OUTLP:    LCI    IDPT,X
    CCN    SLREP
    GOEQ    NOTOU,2,X,X
    LCI    IDPT,R
    GOSUB    MDOUCH,X
NOTOU:    LAV    IDPT,X
    AAL    OF(LCH)
    STV    IDPT,P
    CAV    TEMPT,A
    GONE    OUTLP,-10,X,X
    GO    BSLOOP,-59,X,X


    NB    "Place atom on forwards stack"

SASTAK:    LAV    FFPT,X
    STV    DSTPT,X
    LAV    IDLEN,X
    GOSUB    BUMPFF,501
    LAV    IDPT,X
    STV    SRCPT,X
    LAV    IDLEN,X
    FMOVE
    LAV    ARGLEN,X
    AAV    IDLEN
    STV    ARGLEN,X
    GO    BSLOOP,-71,X,X


    NB    "Action at end of source text"

MNSTOP:    GOSUB    ERMTST,2324
    GO    LOHALT,2761,X,X




    NB    "Action at skip name"

SKIPPO:    GOSUB    ENCALL,587
    LAI    BINFPT,X
    ANDL    4
    STV    MASKSW,X
    LAV    SKIPLV,X
    CAL    0
    GONE    SKDEL,8,X,X
    LAV    NESTLV,X
    STV    SKIPLV,X
    LAI    BINFPT,X
    ANDL    1
    STV    COPDSW,X
    LAI    BINFPT,X
    ANDL    2
    STV    COPTSW,X

    NB    "Skip delimiter"

SKDEL:    GOSUB    SBSTPL,966
    LAV    COPTSW,X
    CAL    0
    GONE    GL1,2,X,X
    LAL    6
    STV    BESTPL,X
GL1:    LAV    BESPT,X
    SAV    IDPT
    AAL    OF(LCH)
    STV    IDLEN,X
    GOSUB    RESSP,931
    GO    SKD1,19,X,C

    NB    "End of skip"

    GOSUB    DECALV,539
    GO    ENSKIP,6,X,C
    LAV    SKIPLV,X
    CAV    NESTLV,X
    GOGR    ENSKIP,3,X,X

    NB    "End of nested skip"

    LAL    4
    STV    MASKSW,X
    GO    BST1,7,X,X

    NB    "End of outermost skip"

ENSKIP:    CLEAR    SKIPLV
    LAV    COPDSW,X
    CAL    0
    GOEQ    BSNEXT,-115,X,X
BSTREX:    GOSUB    SMSKSW,990
    GOSUB    SBSTPL,941
    CLEAR    BESPT
BST1:    LAV    IDLEN,X
    CAL    0
    GOEQ    BSLOOP,-118,X,X
    GO    BSSWIT,-85,X,X


    NB    "Non-closing skip delimiter"

SKD1:    LAV    SKIPLV,X
    CAV    NESTLV,X
    GONE    BSSWIT,-88,X,X
    LAV    COPDSW,X
    CAL    0
    GOEQ    BSLOOP,-125,X,X
    LAV    SKVAL,X
    CAL    0
    GONE    BSLOOP,-128,X,X
    LAV    OPLEV,X
    CAL    0
    GOEQ    MNOUT,-86,X,X
    GO    SASTAK,-73,X,X




    NB    "Action at warning marker"

WARNO:    GOSUB    RESSP,897
    GO    WRN1,0,X,C
WRN1:    LAV    SPT,X
    STV    WNSPT,X
    LAV    IDPT,X
    STV    WNIDPT,X
    CLEAR    IDLEN
    GOSUB    GSATOM,688
    GO    WRNERR,5,X,C
    LAL    7
    STV    BESTPL,X
    LAL    1
    STV    MASKSW,X
    GO    BSNAME,-126,X,X

WRNERR:    LBV    SVARPT
    SBL    OF(3*LNM)
    LAM    0
    CAL    0
    GONE    GL2,1,X,X
    GOSUB    ERSNW,2239
GL2:    LAV    WNIDPT,X
    STV    IDPT,X
    LAV    WNSPT,X
    STV    SPT,P
    SAV    IDPT
    AAL    OF(LCH)
    STV    IDLEN,X
    GO    BSTREX,-48,X,X


    NB    "Action at stop marker"

