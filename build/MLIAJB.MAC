    RL    DEQ,OF(94*LNM+68*LCH)
    CON    1
    CON    0
    CON    8
    HASH    "MCGO"
    CON    0
    CON    OF(4*LCH)
    STR    "MCGO"
    RL    DIF,OF(63*LNM+48*LCH)
    CON    1
    CON    0
    CON    9
    HASH    "MCALTER"
    CON    0
    CON    OF(7*LCH)
    STR    "MCALTER"
    RL    DTO,OF(84*LNM+58*LCH)
    CON    1
    CON    0
    CON    10
    HASH    "MCNOTE"
    CON    0
    CON    OF(6*LCH)
    STR    "MCNOTE"
    RL    DNL,OF(97*LNM+58*LCH)
    CON    1
    CON    0
    CON    11
    HASH    "MCPVAR"
